pipeline {
    agent any
    stages {
        stage ('BUILD Test2') {
            when {
                branch 'test'
            }
            steps {
                echo 'Building Production code'
            }
        
        }
        
        stage ('BUILD DEV1') {
            when {
                branch 'dev'
            }
            steps {
                echo 'Building dev code'
            }
        }
		
		stage ('BUILD qa1') {
            when {
                branch 'qa'
            }
            steps {
                echo 'Building qa code'
            }
        
        }
		
		stage ('BUILD MASTER1') {
            when {
                branch 'main'
            }
            steps {
                echo 'Building Production code'
            }
        
        }
    }
}
